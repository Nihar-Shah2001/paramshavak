FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat     raw;

sets
(
    magE
    {
        type            uniform;
        axis            distance;
        start           (0.000001 -0.05 0);
        end             (0.000001 0 0);
        nPoints         100;
    }
);

surfaces
();

fields
(
    magE
);

